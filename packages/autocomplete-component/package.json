{
  "name": "@aws/amazon-q-developer-cli-autocomplete",
  "version": "0.1.0",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && cp src/index.css dist",
    "build:tailwindcss": "tailwindcss -i src/index.css -o dist/index.css -c tailwind.config.js",
    "dev": "tsc --watch",
    "vite": "vite",
    "lint": "eslint src --report-unused-disable-directives && prettier --check src",
    "lint:fix": "eslint src --fix && prettier --write src",
    "clean": "rm -rf dist/ && rm -rf node_modules/",
    "ab": "tsx scripts/ws.ts"
  },
  "browserslist": [
    "defaults",
    "Safari >= 11"
  ],
  "dependencies": {
    "@aws/amazon-q-developer-cli-api-bindings": "workspace:^",
    "@aws/amazon-q-developer-cli-api-bindings-wrappers": "workspace:^",
    "@aws/amazon-q-developer-cli-autocomplete-parser": "workspace:^",
    "@aws/amazon-q-developer-cli-fuzzysort": "workspace:^",
    "@aws/amazon-q-developer-cli-ipc-backend-core": "workspace:^",
    "@aws/amazon-q-developer-cli-ipc-backend-websocket-mux": "workspace:^",
    "@aws/amazon-q-developer-cli-proto": "workspace:^",
    "@aws/amazon-q-developer-cli-shell-parser": "workspace:^",
    "@fig/autocomplete-helpers": "^1.0.7",
    "@fig/autocomplete-shared": "^1.1.2",
    "@juggle/resize-observer": "^3.4.0",
    "@types/js-yaml": "^4.0.9",
    "@withfig/autocomplete-types": "^1.31.0",
    "deep-object-diff": "^1.1.9",
    "events": "^3.3.0",
    "exponential-backoff": "^3.1.1",
    "js-yaml": "^4.1.0",
    "loglevel": "^1.9.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-shadow": "^20.5.0",
    "react-window": "^1.8.10",
    "semver": "^7.6.3",
    "tailwind-merge": "^2.5.5",
    "use-resize-observer": "^9.1.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@aws/amazon-q-developer-cli-eslint-config": "workspace:^",
    "@aws/amazon-q-developer-cli-shared": "workspace:^",
    "@aws/amazon-q-developer-cli-types": "workspace:^",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.16",
    "@types/react-dom": "^18.3.5",
    "@types/react-window": "^1.8.8",
    "@types/semver": "^7.5.8",
    "@types/ws": "^8.5.13",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.16",
    "terser": "^5.37.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "ws": "^8.18.0"
  }
}
